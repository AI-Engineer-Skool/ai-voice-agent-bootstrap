# Backend configuration
PROVIDER=azure
VOICE_NAME=alloy
CORS_ORIGINS=["http://localhost:5173"]

# Azure OpenAI credentials (required for Azure provider)
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com
AZURE_OPENAI_KEY=your-azure-key
AZURE_OPENAI_API_VERSION=2025-04-01-preview
AZURE_OPENAI_MODERATOR_DEPLOYMENT=your-chat-deployment-name

# Optional override if you use a different realtime deployment name
# REALTIME_MODEL=gpt-realtime

# Required WebRTC gateway URL for your realtime deployment
AZURE_OPENAI_REALTIME_ENDPOINT=https://your-resource.openai.azure.com/openai/realtime
